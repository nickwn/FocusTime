<!DOCTYPE html>
<html>
  <head>
    <title>Focus Time</title>
    <link rel="stylesheet" type="text/css" href="../style.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script src="https://kit.fontawesome.com/522d0f159e.js" crossorigin="anonymous"></script>

    <!-- Google Analytics -->
    <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-159601585-1', 'auto');
    ga('send', 'pageview');
    ga('require', 'GTM-MLPBVKK');
    </script>
    <!-- End Google Analytics -->
  </head>
  <body>
    {{> header }}
    {{#each sessions}}
    <a href="/event/{{name}}">
        <div class="event-card">
            <img src="/media/chevron.png" alt="Go to timer">
            <h2>{{name}}</h2>
            <h3><i class="fas fa-check"></i> {{reward}}, <i class="fas fa-times"></i> {{consequence}}</h3>
        </div>
    </a>
    {{/each}}
    <div class="new-event-btn">
        <a href="/new"><img src="/media/plus.png" alt="Create new event"></a>
    </div><div class="calendar-btn">
        <a href="/calendar.html"><img src="/media/cal.png" alt="Go to calendar"></a>
    </div>

  </body>
